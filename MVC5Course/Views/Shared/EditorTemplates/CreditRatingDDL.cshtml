@model double?
@using MVC5Course.Models;
@{ 

    FabricsEntities db = new FabricsEntities();
    var rating_list = (from p in db.Client select p.CreditRating).Distinct().
        OrderBy(p => p).ToList();
    var CreditRating = new SelectList(rating_list, Model);

}

@Html.DropDownList("", CreditRating, "--請選擇 Rating--")


